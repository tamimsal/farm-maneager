# Production Environment Variables for Farm Manager
# Copy this file to .env.production and update the values

# WebSocket URL for production (HTTPS)
# Replace 'your-company-domain.com' with your actual domain
REACT_APP_WS_URL=wss://your-company-domain.com/ws

# Disable source maps for production (security)
GENERATE_SOURCEMAP=false

# Optional: API URL if you add backend services later
# REACT_APP_API_URL=https://your-company-domain.com/api

# Optional: Environment identifier
REACT_APP_ENV=production

# Optional: Enable production logging
REACT_APP_DEBUG=false
